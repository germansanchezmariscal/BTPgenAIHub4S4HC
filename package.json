{
  "name": "German_1_STUDENTH01",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-ai-sdk/foundation-models": "^1.15.0",
    "@sap-ai-sdk/orchestration": "^1.15.0",
    "@sap-cloud-sdk/http-client": "^4.0.2",
    "@sap-cloud-sdk/util": "^4.0.2",
    "@sap/cds": "^8",
    "@sap/low-code-event-handler": "^1.2.5",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^4.8.0",
    "express": "^4",
    "hdb": "^0.19.12",
    "passport": "^0.7.0",
    "@sap-cloud-sdk/connectivity": "^3",
    "@sap-cloud-sdk/resilience": "^3"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@sap/dev-cap-tools": "^1.31.0"
  },
  "scripts": {
    "start": "cds-serve",
    "start-local": "npm run generate-entry-point && npm run cds-watch",
    "generate-entry-point": "dev-cap-tools gen-entrypoint",
    "cds-watch": "cds watch --open",
    "watch-german_1_studenth01.customermessages": "cds watch --open german_1_studenth01.customermessages/webapp/index.html?sap-ui-xx-viewCache=false",
    "watch-german_1_studenth01.productfaqapp": "cds watch --open german_1_studenth01.productfaqapp/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "auth": {
        "[development]": {
          "kind": "dummy"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      },
      "S4HCP_ServiceOrder_Odata": {
        "kind": "odata-v2",
        "csrf": true,
        "csrfInBatch": true,
        "model": "srv/external/S4HCP_ServiceOrder_Odata",
        "[production]": {
          "credentials": {
            "destination": "S4HCP-ServiceOrder-Odata"
          }
        }
      }
    }
  },
  "lcap": true,
  "sapux": [
    "app/german_1_studenth01.customermessages",
    "app/german_1_studenth01.productfaqapp"
  ]
}
